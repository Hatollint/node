{
  "name": "miniprofiler",
  "version": "0.0.9",
  "description": "A simple but effective mini-profiler.",
  "main": "lib/miniprofiler.js",
  "scripts": {
    "lint": "eslint .",
    "stop-services": "docker-compose stop",
    "test": "docker-compose up test",
    "coverage": "docker-compose up coverage",

    "mocha": "mocha tests/ -c",
    "mocha-coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- tests/ -R spec",
    "check-coverage": "istanbul check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "update-coveralls": "cat coverage/lcov.info | sed s/.usr.src.miniprofiler/./ | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MiniProfiler/node.git"
  },
  "bugs": {
    "url": "http://github.com/MiniProfiler/node/issues"
  },
  "author": {
    "name": "Guilherme Oenning",
    "email": "oenning.ti@gmail.com",
    "url": "http://goenning.net/"
  },
  "contributors": [
    "Matt Jibson <matt.jibson@gmail.com> (https://mattjibson.com/)",
    "Kevin Montrose"
  ],
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "dependencies": {
    "debug": "^2.2.0",
    "node-uuid": "^1.4.7",
    "underscore": "^1.8.3"
  },
  "tags": [
    "profiler",
    "sql",
    "performance",
    "profiling",
    "timing"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.0",
    "express": "^4.13.4",
    "hapi": "^13.4.1",
    "istanbul": "^0.4.3",
    "koa": "^1.2.0",
    "koa-route": "^2.4.2",
    "mocha": "^2.5.3",
    "pg": "^4.5.5",
    "pug": "^2.0.0-alpha7",
    "redis": "^2.6.0-2",
    "request": "^2.72.0"
  }
}
