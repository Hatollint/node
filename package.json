    "Matt Jibson <matt.jibson@gmail.com> (https://mattjibson.com/)",
    "Kevin Montrose"
  ],
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "dependencies": {
    "debug": "^2.2.0",
    "node-uuid": "^1.4.7",
    "underscore": "^1.8.3"
  },
  "tags": [
    "profiler",
    "performance",
    "profiling",
    "timing"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.0",
