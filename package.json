{
  "name": "miniprofiler",
  "version": "0.0.9",
  "description": "A simple but effective mini-profiler.",
  "main": "lib/miniprofiler.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha tests/ -c",
    "test:watch": "mocha tests/ -c --watch",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- tests/ -R spec",
    "check-coverage": "istanbul check-coverage --statements 70 --branches 70 --functions 70 --lines 70",
    "update-coveralls": "cat coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MiniProfiler/node.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "dependencies": {
    "debug": "^2.2.0",
    "node-uuid": "^1.4.7",
    "underscore": "1.5.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "express": "^4.13.4",
    "hapi": "^13.4.1",
    "istanbul": "^0.4.3",
    "koa": "^1.2.0",
    "koa-route": "^2.4.2",
    "mocha": "^2.4.5",
    "pg": "^4.5.5",
    "pug": "^2.0.0-alpha7",
    "redis": "^2.6.0-2",
    "request": "^2.72.0"
  }
}
